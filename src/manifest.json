{
  "manifest_version": 2,

  "name": "Toggl-Jira-Sync",
  "description": "Migrate Toggl time entries into Jira as Worklogs.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "https://*/",
    "storage"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}